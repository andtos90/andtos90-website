    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^9.2.4",
    "react-markdown": "4.0.3",
    "react-router": "^4.2.0",
    "react-static": "5.9.12"
  },
  "devDependencies": {
    "babel-plugin-emotion": "^9.2.4",
    "emotion-server": "^9.2.4",
    "eslint-config-react-tools": "1.x.x",
    "flow-bin": "0.82.0",
    "husky": "1.1.1",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "serve": "^6.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
